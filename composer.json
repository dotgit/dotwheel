{
  "name": "dotgit/dotwheel",
  "description": "PHP framework providing lightweight solutions for most frequent development tasks",
  "keywords": ["php", "framework", "db", "cache", "http", "nls", "html", "bootstrap", "ui", "acl", "recaptcha", "mime", "crypt", "utf-8"],
  "license": "BSD-2-Clause",
  "type": "library",
  "readme": "README.md",
  "autoload": {
    "classmap": [
      "cache/",
      "db/",
      "http/",
      "nls/",
      "ui/",
      "util/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "bin": ["nls/po2php.php"]
}
